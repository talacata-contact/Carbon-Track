    "deploy:prod": "firebase deploy --only functions:api_prod,functions:dailyUserNotifications_prod",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "main": "index.mjs",
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^17.2.1",
    "expo-server-sdk": "^4.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}
