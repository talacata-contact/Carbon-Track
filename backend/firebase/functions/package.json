{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "license": "Apache-2.0",
  "author": "TALACATA",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "emulate:preprod": "firebase emulators:start --only functions:api_preprod,functions:dailyUserNotifications_preprod",
